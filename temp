Recommend ?user ?song.REC ?RATING USING CB
WHERE {
    ?user rated ?rating.
    ?rating song ?song.
    ?rating score ?Rscore.
    ?song genre ?genre.
}
BASED ON {
    ?song genre ?genre.
}
SETTINGS{
    ?user rdf:type recsparq:User.
    ?song rdf:type recsparql:Item.
    ?rating rdf:type recsparql:UserRating.
}